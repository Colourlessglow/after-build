{
  "name": "@whitekite/vite-plugin-after-build",
  "version": "0.0.26",
  "license": "MIT",
  "author": {
    "name": "whitekite",
    "email": "1075790909@qq.com"
  },
  "sideEffects": false,
  "homepage": "https://gitee.com/white-kite/after-build/blob/master/README.md",
  "repository": "https://gitee.com/white-kite/after-build",
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch",
    "release": "standard-version"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@whitekite/unplugin-after-build": "workspace:*"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true,
      "changelog": true
    }
  }
}
