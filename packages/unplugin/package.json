{
  "name": "@whitekite/unplugin-after-build",
  "version": "0.0.20",
  "license": "MIT",
  "author": {
    "name": "whitekite",
    "email": "1075790909@qq.com"
  },
  "sideEffects": false,
  "repository": "Colourlessglow/after-build",
  "scripts": {
    "build": "unbuild",
    "release": "standard-version"
  },
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./vite": {
      "import": "./vite.mjs",
      "require": "./vite.js",
      "types": "./vite.d.ts"
    },
    "./webpack": {
      "import": "./webpack.mjs",
      "require": "./webpack.js",
      "types": "./webpack.d.ts"
    }
  },
  "dependencies": {
    "unplugin": "^0.6.2",
    "@whitekite/after-build": "workspace:*"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true,
      "changelog": true
    }
  }
}
